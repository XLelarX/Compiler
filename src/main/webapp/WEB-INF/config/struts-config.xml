<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
        "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
    <form-beans>
        <form-bean name="CompilerForm" type="compiler.lelar.compiler.CompilerForm"/>
    </form-beans>
    <action-mappings>
        <action path="/compiler"
                type="compiler.lelar.compiler.CompilerAction"
                name="CompilerForm"
                validate="false">
            <forward name="Start" path="/WEB-INF/jsp/compiler.jsp"/>
        </action>
        <action path="/compileAjax"
                type="controller.CompileRestService"
                name="CompilerForm"
                validate="false">
        </action>
        <action path="/sendVars"
                type="controller.SendVarsRestService"
                name="CompilerForm"
                validate="false">
        </action>
        <action path="/getResult"
                type="controller.GetResultRestService"
                name="CompilerForm"
                validate="false">
        </action>
    </action-mappings>
</struts-config>